{
	"name": "livestreamapp",
	"version": "0.0.1",
	"private": true,
	"license": "MIT license",
	"scripts": {
		"inst": "yarn install && react-native link",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"all": "rm -rf node_modules && yarn install && cd ios && rm -rf Pods Podfile.lock && arch -x86_64 pod install",
		"clean-android": "cd android && set ENVFILE=.env.dev && ./gradlew clean",
		"build-android": "cd android && npx gradlew assembleRelease",
		"reset-cache": "react-native start --reset-cache",
		"android-dev": "set ENVFILE=.env.dev && npx react-native run-android",
		"android-dev-macos": "set ENVFILE=.env && npx react-native run-android",
		"android-prod": "set ENVFILE=.env.prod && npx react-native run-android",
		"apk-dev": "cd android && set ENVFILE=.env.dev && ./gradlew assembleRelease",
		"apk-dev-macos": "cd android && set ENVFILE=.env && ./gradlew assembleRelease",
		"clean-android-macos": "cd android && set ENVFILE=.env && ./gradlew clean",
		"apk-prod": "cd android && set ENVFILE=.env.prod && ./gradlew assembleRelease",
		"ios-dev": "npx react-native run-ios --scheme livestreamapp-Dev",
		"ios-prod": "npx react-native run-ios --scheme livestreamapp-Prod",
		"update-android": "appcenter codepush release-react -a TaiTran2512/Livestream-App -d Production",
		"update-ios": "appcenter codepush release-react -a TaiTran2512/Virtually-Everywhere-App -d Production",
		"list-key-ios": "appcenter codepush deployment list --app TaiTran2512/Virtually-Everywhere-App -k",
		"list-key-android": "appcenter codepush deployment list --app TaiTran2512/Livestream-App -k"
	},
